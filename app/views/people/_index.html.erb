<% @people.order('id DESC').each do |person| %>
  <tr>
    
    <td><%= person.name %></td>
			<td><%= person.address %></td>
			<td><%= person.phone %></td>
			<td><%= person.email_address %></td>
			<td><%= person.about %></td>

    <td><%= link_to "Edit", edit_person_path(person), remote: true%></td>
    <td><%= link_to "Delete", person, method: :delete, remote: true%></td>

  </tr>
<% end %>